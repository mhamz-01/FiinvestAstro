            <video 
            class="absolute inset-0 w-full h-full object-cover transform scale-105" 
            autoplay 
            muted 
            loop 
            playsinline 
            poster="images/dummy.jpg"
        >
            <source 
                src="videos/fiinvestHeroSectionVideo.mp4" 
                type="video/mp4" 
                media="(min-width: 768px)"
            />
        </video>
        
        
            <!-- Enhanced Gradient Overlay -->
            <div class="absolute inset-0 bg-gradient-to-b from-black/80 via-black/60 to-black/80"></div>
        
            <!-- Centered Content Section -->
            <div class="relative min-h-screen flex items-center justify-center px-4 md:px-0">
